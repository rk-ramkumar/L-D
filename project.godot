; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Ludo"
config/tags=PackedStringArray("main")
run/main_scene="res://Scenes/2D/play_ground.tscn"
config/features=PackedStringArray("4.1", "Mobile")
config/icon="res://icon.svg"

[autoload]

Helpers="*res://Scripts/Helpers.gd"
GameManager="*res://Scripts/GameManager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="floor"
3d_physics/layer_1="floor"
2d_physics/layer_2="wall"
3d_physics/layer_2="die"
2d_physics/layer_3="player"
3d_physics/layer_3="eren"

[rendering]

textures/canvas_textures/default_texture_filter=3
renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
textures/lossless_compression/force_png=true
2d/snap/snap_2d_vertices_to_pixel=true
